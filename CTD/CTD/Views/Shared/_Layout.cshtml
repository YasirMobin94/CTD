        <script type="text/javascript" src="~/bundles/js/validate-nav-tool.min.js" asp-append-version="true" defer></script>

        <link rel="preload" src="~/bundles/js/jquery-ui.min.js" asp-append-version="true" as="script">
        <script type="text/javascript" src="~/bundles/js/jquery-ui.min.js" asp-append-version="true" defer></script>

        <link rel="preload" src="~/bundles/js/jquery-bootstrap-touchspin.min.js" asp-append-version="true" as="script">
