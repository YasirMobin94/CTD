@using Microsoft.AspNetCore.Http.Extensions;
@{
    var url = Context.Request.GetDisplayUrl();
}
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Google Tag Manager -->
    @if (url.Contains("creativethinkdigital.co.uk"))
    {
        <script type="text/javascript" async>
            setTimeout(() => {
                console.log("gtm script loaded");
                (function (w, d, s, l, i) {
                    w[l] = w[l] || [];
                    w[l].push({
                        'gtm.start': new Date().getTime(),
                        event: 'gtm.js'
                    });
                    var f = d.getElementsByTagName(s)[0],
                        j = d.createElement(s),
                        dl = l != 'dataLayer' ? '&l=' + l : '';
                    j.async = true;
                    j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl;
                    f.parentNode.insertBefore(j, f);
                })(window, document, 'script', 'dataLayer', 'GTM-M7WFFHSH')
            }, 5000)
        </script>
    }
    <!-- End Google Tag Manager -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, maximum-scale=5.0">
    <!--[if lt IE 9]><script src="js/respond.js"></script><![endif]-->

    <title>@ViewBag.Title | Creative Think Digital</title>
    <meta name="title" content="@ViewBag.Title" />
    <meta name="description" content="@ViewBag.Description" />




    <environment include="Development">
        <link href="~/css/bootstrap.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/font-awesome.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/flaticon.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/animate.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/owl.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/animation.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/jquery-ui.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/custom-animate.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/jquery.bootstrap-touchspin.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/jquery.fancybox.min.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/jquery.mCustomScrollbar.min.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/style.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/responsive.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/site.css" rel="stylesheet" asp-append-version="true">
    </environment>

    <environment exclude="Development">
        <link rel="preload" href="~/bundles/css/bootstrap.min.css" asp-append-version="true" as="style">
        <link href="~/bundles/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/font-icon.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/font-icon.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/animate.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/animate.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/owl.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/owl.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/animation.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/animation.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/custom-animate.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/custom-animate.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/jquery-ui.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/jquery-ui.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/jquery-plugins.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/jquery-plugins.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/style.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/style.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/responsive.min.css" rel="preload" asp-append-version="true" as="style">
        <link href="~/bundles/css/responsive.min.css" rel="stylesheet" asp-append-version="true" async>

        <link href="~/bundles/css/site.min.css" rel="preload" asp-append-version="true" as="style">
