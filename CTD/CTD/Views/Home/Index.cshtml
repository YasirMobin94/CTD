@{
    ViewBag.Title = "IT Staff Augmentation & Web Development Company";
    ViewBag.Description = "Unlock unparalleled growth and efficiency with our specialized IT staff augmentation services. Explore the future of workforce augmentation and SEO excellence today!";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Banner Section Two -->
<section class="banner-section-two">
    @*<div class="pattern-layer-one" style="background-image: url(images/main-slider/pattern-4.png)"></div>*@
    <div class="pattern-layer-two" style="background-image: url(images/main-slider/pattern-5.webp)"></div>
    <div class="pattern-layer-three" style="background-image: url(images/main-slider/pattern-6.webp)"></div>
    <div class="main-slider-carousel owl-carousel owl-theme">

        <div class="slide">
            <div class="auto-container">
                <div class="row clearfix">

                    <!-- Image Column -->
                    <div class="image-column col-lg-6 col-md-12 col-sm-12">
                        <div class="inner-column">
                            <div class="image">
                                <img loading="lazy" src="~/images/main-slider/header.webp" alt="" />
                            </div>
                        </div>
                    </div>

                    <!-- Content Column -->
                    <div class="content-column col-lg-6 col-md-12 col-sm-12">
                        <div class="inner-column">
                            <h1>IT Staff Augmentation <br> & Web Development Company</h1>
                            <div class="text">Welcome to Creative Think Digital, a leading digital solutions provider specializing in Graphic Designing, IT staff augmentation, search engine optimization, and web development. </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>

        <div class="slide">
            <div class="auto-container">
                <div class="row clearfix">

                    <!-- Image Column -->
                    <div class="image-column col-lg-6 col-md-12 col-sm-12">
                        <div class="inner-column">
                            <div class="image">
                                <img loading="lazy" src="~/images/main-slider/566x766.webp" alt="" />
                            </div>
                        </div>
                    </div>

                    <!-- Content Column -->
                    <div class="content-column col-lg-6 col-md-12 col-sm-12">
                        <div class="inner-column">
                            <h1>SEO and <br> Graphic Designing Company</h1>
                            <div class="text">Our passion for creativity and innovation drives us to deliver exceptional digital experiences for businesses of all sizes.</div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>
<section class="experiance-section" style="background-image: url(/images/background/pattern-9.webp)">
    <div class="auto-container">
        <div class="sec-title centered">
            <div class="title">EXPERIENCE. EXECUTION. EXCELLENCE.</div>
            <h2>What We Actually Do</h2>
        </div>
        <div class="experiance-info-tabs">
            <div class="experiance-tabs tabs-box">

                <!-- Tab Btns-->
                <ul class="tab-btns tab-buttons clearfix">
                    <li data-tab="#prod-coding" class="tab-btn"><span class="icon flaticon-coding"></span>Web Development</li>
                    <li data-tab="#prod-it-staff" class="tab-btn"><span class="icon flaticon-user"></span>IT Staff Augmentation</li>
                    <li data-tab="#prod-seo" class="tab-btn active-btn"><span class="icon flaticon-seo"></span>SEO</li>
                    <li data-tab="#prod-graphic-design" class="tab-btn"><span class="icon icon flaticon-designer-1"></span>Graphic Designing</li>
                </ul>

                <!--Tabs Container-->
                <div class="tabs-content">

                    <!--Tab / Active Tab-->
                    <div class="tab" id="prod-coding">
                        <div class="content">
                            <h4>Web Development Services</h4>
                            <div class="text">Our web development team crafts responsive and user-friendly websites tailored to your unique business needs. <br />We build robust, scalable, and high-performing websites to leave a lasting impression on your visitors.</div>
                        </div>
                    </div>

                    <!-- Tab -->
                    <div class="tab" id="prod-it-staff">
                        <div class="content">
                            <h4>IT Staff Augmentation</h4>
                            <div class="text">Scale your team effortlessly with our IT staff augmentation services. We provide highly skilled and vetted professionals to complement your projects and drive success.</div>
                        </div>
                    </div>

                    <!-- Tab -->
                    <div class="tab active-tab" id="prod-seo">
                        <div class="content">
                            <h4>Search Engine Optimization (SEO)</h4>
                            <div class="text">
                                Improve your online visibility and drive organic traffic with our data-driven SEO strategies. <br> Our team of SEO experts ensures your website ranks higher in search engine results, reaching the right audience at the right time.
                            </div>
                        </div>
                    </div>

                    <!-- Tab -->
                    <div class="tab" id="prod-graphic-design">
                        <div class="content">
                            <h4>Graphic Designing</h4>
                            <div class="text">Our talented graphic designers blend aesthetics with functionality to create visually stunning and impactful designs that resonate with your target audience.</div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>
<!-- End Experiance Section -->
<!-- Counter Section -->
<section class="counter-section">
    <div class="auto-container">
        <div class="inner-container">
            <!-- Fact Counter -->
            <div class="fact-counter">
                <div class="row clearfix">

                    <!-- Column -->
                    <div class="column counter-column col-lg-3 col-md-6 col-sm-12">
                        <div class="inner wow fadeInLeft" data-wow-delay="0ms" data-wow-duration="1500ms">
                            <div class="content">
                                <div class="count-outer count-box">
                                    <span class="count-text" data-speed="3000" data-stop="18">0</span>+
                                </div>
                                <h4 class="counter-title">ACTIVE CLIENTS</h4>
                            </div>
                        </div>
                    </div>

                    <!-- Column -->
                    <div class="column counter-column col-lg-3 col-md-6 col-sm-12">
                        <div class="inner wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="1500ms">
                            <div class="content">
                                <div class="count-outer count-box alternate">
                                    <span class="count-text" data-speed="5000" data-stop="70">0</span>+
                                </div>
                                <h4 class="counter-title">PROJECTS DONE</h4>
                            </div>
                        </div>
                    </div>

                    <!-- Column -->
                    <div class="column counter-column col-lg-3 col-md-6 col-sm-12">
                        <div class="inner wow fadeInLeft" data-wow-delay="600ms" data-wow-duration="1500ms">
                            <div class="content">
                                <div class="count-outer count-box">
                                    <span class="count-text" data-speed="2000" data-stop="10">0</span>+
                                </div>
                                <h4 class="counter-title">TEAM ADVISORS</h4>
                            </div>
                        </div>
                    </div>

                    <!-- Column -->
                    <div class="column counter-column col-lg-3 col-md-6 col-sm-12">
                        <div class="inner wow fadeInLeft" data-wow-delay="900ms" data-wow-duration="1500ms">
                            <div class="content">
                                <div class="count-outer count-box">
                                    <span class="count-text" data-speed="3500" data-stop="2">0</span>+
                                </div>
                                <h4 class="counter-title">GLORIOUS YEARS</h4>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Counter Section -->
<!-- About Section Two -->
<section class="about-section-two" style="background-image: url(images/background/about-section-two.webp)">
    <div class="auto-container">
        <div class="row clearfix">

            <!-- Image Column -->
            <div class="image-column col-lg-7 col-md-12 col-sm-12">
                <div class="inner-column">
                    <div class="circle-layer" style="background-image: url(/images/background/pattern-10.png)"></div>
                    <div class="image">
                        <img loading="lazy" src="/images/resource/about.webp" alt="" />
                    </div>
                </div>
            </div>

            <!-- Skill Column -->
            <div class="skill-column col-lg-5 col-md-12 col-sm-12">
                <div class="inner-column">

                    <!-- Sec Title -->
                    <div class="sec-title light">
                        <div class="title">About CTD</div>
                        <h2>Transform Your Business With The Creative Think Digital.</h2>
                        <div class="text">We listen. We advise. We design, together. Happy customers and ongoing relationships are what we strive for. Success is measured by results, the most important being how our clients feel about their experience with us.</div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>
<!-- End About Section Two -->
<!-- Process Section -->
<section class="process-section">
    <div class="pattern-layer-one" style="background-image: url(/images/background/pattern-11.jpg)"></div>
    <div class="pattern-layer-two" style="background-image: url(/images/background/pattern-12.jpg)"></div>
    <div class="auto-container">
        <!-- Sec Title -->
        <div class="sec-title centered">
            <div class="title"></div>
            <h2>Why Partner With Us</h2>
        </div>
        <div class="row clearfix">

            <!-- Process Block -->
            <div class="process-block col-lg-4 col-md-6 col-sm-12">
                <div class="inner-box wow fadeInLeft" data-wow-delay="0ms" data-wow-duration="1500ms">
                    <div class="number-box">01</div>
                    <h4><a href="#">Timely Delivery</a></h4>
                    <div class="text">We understand the value of time, and our team is committed to delivering projects on schedule without compromising on quality.</div>
                </div>
            </div>

            <!-- Process Block -->
            <div class="process-block col-lg-4 col-md-6 col-sm-12">
                <div class="inner-box wow fadeInUp" data-wow-delay="0ms" data-wow-duration="1500ms">
                    <div class="number-box">02</div>
                    <h4><a href="#">Client Success Stories</a></h4>
                    <div class="text">
                        We have a proven track record of delivering successful digital projects for diverse industries, earning the trust of our clients.
                    </div>
                </div>
            </div>

            <!-- Process Block -->
            <div class="process-block col-lg-4 col-md-6 col-sm-12">
                <div class="inner-box wow fadeInRight" data-wow-delay="0ms" data-wow-duration="1500ms">
                    <div class="number-box">03</div>
                    <h4><a href="#">Transparent Communication</a></h4>
                    <div class="text">We believe in open and transparent communication throughout the project lifecycle. You'll be kept informed at every step of the process.</div>
                </div>
            </div>

        </div>
    </div>
</section>
<!-- End Process Section -->

<partial name="~/Views/Shared/Partials/_AwardsAndBadges.cshtml" />

<!-- Appointment Section -->
<section class="appointment-section">
    <div class="image-layer" style="background-image: url(images/background/4.jpg)"></div>
    <div class="auto-container">
        <!-- Sec Title -->
        <div class="sec-title light centered">
            <div class="title">CONTACT US</div>
            <h2>Join Us To Get IT Free <br> Consultations</h2>
        </div>
        <div class="inner-container">
            <div class="row clearfix">

                <!-- Image Column -->
                <div class="image-column col-lg-6 col-md-12 col-sm-12">
                    <div class="inner-column wow fadeInLeft" data-wow-delay="0ms" data-wow-duration="1500ms">
                        <div class="image">
                            <img loading="lazy" src="~/images/resource/about-appointment.webp" alt="" />
                        </div>
                    </div>
                </div>

                <!-- Form Column -->
                <div class="form-column col-lg-6 col-md-12 col-sm-12">
                    <div class="inner-column">
                        <h4>You Don't Hesitate To Contact <br> With Us, Now Say Hello......</h4>
                        <!-- Appointment Form -->
                        <div class="appointment-form">
                            <partial name="~/Views/Shared/Partials/_HomeContactForm.cshtml" />

                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>
</section>


<script>
    $(document).ready(function () {
        CTDNav.AddCurrentActiveClass(`home`)
    })
</script>